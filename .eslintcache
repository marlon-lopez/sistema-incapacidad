ntainer>\r\n      <h1>Login</h1>\r\n      {state.error && <h3>{state.error}</h3>}\r\n      <StyledFormCard onSubmit={submitHandler}>\r\n        <label>Correo Electronico</label>\r\n        <input\r\n          type='text'\r\n          name='email'\r\n          id='email'\r\n          required\r\n          onChange={(e) => inputChangeHandler(e, userData, setUserData)}\r\n        />\r\n\r\n        <label>Contraseña</label>\r\n        <input\r\n          type='password'\r\n          name='password'\r\n          id='password'\r\n          required\r\n          onChange={(e) => inputChangeHandler(e, userData, setUserData)}\r\n        />\r\n\r\n        <button>Iniciar sesion</button>\r\n        <p>{state.isAuthenticated === true ? state.user.name : ' '}</p>\r\n        <Link to='/register'> Register</Link>\r\n      </StyledFormCard>\r\n    </LoginContainer>\r\n  )\r\n}\r\nconst LoginContainer = styled.div`\r\n  min-height: 100vh;\r\n  width: 90%;\r\n  margin: 0 auto;\r\n  h1 {\r\n    text-align: center;\r\n  }\r\n  a {\r\n    text-decoration: none;\r\n    color: #ea4c89;\r\n  }\r\n`\r\nconst StyledFormCard = styled.form`\r\n  background: white;\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 1fr);\r\n  grid-gap: 20px;\r\n  padding: 30px 10px;\r\n  text-align: center;\r\n\r\n  label {\r\n    color: #323232;\r\n    grid-column: 1/3;\r\n  }\r\n\r\n  input {\r\n    font-size: 1em;\r\n    padding: 3px 5px;\r\n    color: #707e93;\r\n    border: solid 1px rgba(187, 187, 187, 0.8);\r\n    border-radius: 3px;\r\n  }\r\n  button {\r\n    background: #4b74ff;\r\n    border: none;\r\n    border-radius: 5px;\r\n    color: white;\r\n    grid-row: 3/4;\r\n    grid-column: 2/3;\r\n    padding: 10px 0;\r\n  }\r\n  a {\r\n    grid-column: 3/4;\r\n    align-self: center;\r\n  }\r\n`\r\n\r\nexport default Login\r\n","C:\\Users\\marlo\\OneDrive\\Escritorio\\Projects\\sistema-incapacidad\\src\\components\\User.js",["96"],"C:\\Users\\marlo\\OneDrive\\Escritorio\\Projects\\sistema-incapa