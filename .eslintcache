t) => {\r\n    event.preventDefault()\r\n    updateUser(dispatch, state.token, newUserInfo)\r\n  }\r\n\r\n  useEffect(() => {\r\n    console.log(userInfo)\r\n    console.log(state)\r\n    getUserReq(state.token)\r\n    return\r\n  }, [state])\r\n  return (\r\n    <div>\r\n      {userInfo && (\r\n        <div className='profile'>\r\n          <h4>{userInfo._id}</h4>\r\n          <h4>{userInfo.name}</h4>\r\n          <h4>{userInfo.email}</h4>\r\n          <h4>{userInfo.job}</h4>\r\n          <h4>{userInfo.dui}</h4>\r\n          <h4>{userInfo.code}</h4>\r\n          <h4>{userInfo.startDate}</h4>\r\n        </div>\r\n      )}\r\n      {newUserInfo && (\r\n        <div className='edit-form'>\r\n          <form onSubmit={submitHandler}>\r\n            <label>\r\n              Email Address\r\n              <input\r\n                type='email'\r\n                name='email'\r\n                id='email'\r\n                value={newUserInfo.email}\r\n                onChange={(e) => {\r\n                  userChangeHandler({ email: e.target.value })\r\n                }}\r\n              />\r\n            </label>\r\n            <label>\r\n              Name\r\n              <input\r\n                type='text'\r\n                name='name'\r\n                id='name'\r\n                value={newUserInfo.name}\r\n                onChange={(e) => {\r\n                  userChangeHandler({ name: e.target.value })\r\n                }}\r\n              />\r\n            </label>\r\n            <label>\r\n              Job\r\n              <input\r\n                type='text'\r\n                name='job'\r\n                id='job'\r\n                value={newUserInfo.job}\r\n                onChange={(e) => {\r\n                  userChangeHandler({ job: e.target.value })\r\n                }}\r\n              />\r\n            </label>\r\n            <label>\r\n              DUI\r\n              <input\r\n                type='text'\r\n                name='dui'\r\